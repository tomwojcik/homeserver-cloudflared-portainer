{
  "type": 1,
  "title": "Gluetun VPN",
  "description": "Secure VPN container for routing download services through VPN",
  "categories": ["VPN", "Security"],
  "platform": "linux",
  "logo": "https://raw.githubusercontent.com/qdm12/gluetun/master/doc/logo.png",
  "repository": {
    "url": "https://github.com/qdm12/gluetun",
    "stackfile": "stacks/gluetun/docker-compose.yml"
  },
  "env": [
    {
      "name": "VPN_SERVICE_PROVIDER",
      "label": "VPN Service Provider",
      "description": "VPN service provider (e.g., surfshark, nordvpn, expressvpn)",
      "default": "surfshark"
    },
    {
      "name": "VPN_TYPE", 
      "label": "VPN Type",
      "description": "VPN protocol type",
      "default": "wireguard",
      "select": [
        {
          "text": "WireGuard",
          "value": "wireguard"
        },
        {
          "text": "OpenVPN",
          "value": "openvpn"
        }
      ]
    },
    {
      "name": "WIREGUARD_PRIVATE_KEY",
      "label": "WireGuard Private Key",
      "description": "Your WireGuard private key"
    },
    {
      "name": "WIREGUARD_ADDRESSES",
      "label": "WireGuard Addresses", 
      "description": "WireGuard IP addresses"
    },
    {
      "name": "SERVER_DOMAIN",
      "label": "Server Domain",
      "description": "Your domain name (e.g., example.com)"
    },
    {
      "name": "TZ",
      "label": "Timezone",
      "default": "UTC"
    }
  ]
}